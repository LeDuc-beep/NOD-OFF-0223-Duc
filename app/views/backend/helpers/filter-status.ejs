<% filterStatusHelper = (statusFilter, searchValue, types = "items") => { %>
  <% statusFilter.forEach((value) => {
    let link = `/admin/${types}/status/` + value.value;
    if(searchValue !== "")
    {
      link += "?search=" + searchValue;
    }
  %>
    <a href=<%= link %>>
      <button type="button" class="btn <%= value.class %>">
        <%= value.name %> (<%= value.count %>)
      </button>
    </a>
    <% }) %>
<% } %>
